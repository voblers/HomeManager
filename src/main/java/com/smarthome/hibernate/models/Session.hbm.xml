<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class dynamic-insert="false" dynamic-update="false" mutable="true" name="com.smarthome.hibernate.models.Session" optimistic-lock="version" polymorphism="implicit" select-before-update="false"/>
    <meta attribute="class-description">
        This class contains the session information.
    </meta>
    <id name="sessionID" type="string" column="session_id"></id>
    <property name="sessionRqStart" column="session_rq_start" type="long"></property>
    <property name="sessionRqEnd" column="session_rq_end" type="long"></property>
    <property name="sessionIP" column="session_ip" type="string"></property>
    <property name="sessionPort" column="session_port" type="int"></property>
    <property name="sessionReqURL" column="session_rq_url" type="string"></property>
</hibernate-mapping>
